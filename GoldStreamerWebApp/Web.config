    <add key="PrimaryCulture" value="ar-EG" /> 
    <add key="SecondaryCulture" value="en-US" />
    <add key="DefaultPageSize" value="10" />
    <add key="MaxFailedCount" value="3" />
    <add key="ValidTokenHours" value="0.05" />
    <add key="MainCategoriesNumber" value="10" />
    <add key="CategoriesNumber" value="10" />
    <add key="NewsOrder" value="10" />
    <add key="MaxNewsHome" value="3" />
    <add key="NewsPaging" value="5" />
    <add key="DollarSellPrice" value="7.83"/>
    <add key="DollarBuyPrice" value="7.83"/>
