<div class="mainContainer" id="wrap" style="min-height: 200px; overflow: auto;">

    <div style="width: 100%; min-height: 600px; overflow: auto;">


        <div class="innerData" style="width: 100%;">
            <a href="~/EditProfile">
                <div class="dash_box" >
                    <img src="~/Content/Images/info_icon.png" />
                   
                </div>
            </a>

            <a href="~/Pricer">
                <div class="dash_box" >
                    <img src="~/Content/Images/price_offers_icon.png" />
                    
                </div>
            </a>

            <a href="~/FavorateList/AssignTraders">
                <div class="dash_box">
                    <img src="~/Content/Images/fav_icon.png" />
                </div>
            </a>

            <a href="~/Feedback">
                <div class="dash_box" >
                    <img src="~/Content/Images/msg_icon.png" />
                </div>
            </a>

        </div>
    </div>
</div>

@section SideMenu{
 @Html.Partial("_TraderSideMenu")
}

@section UpperBanner{


    <div class="innerBanner">
        <h1 class="banner_header">@Resources.General.Dashboard</h1>

        <div class="banner"><img src="../../Content/Images/inner_banner.jpg" width="1050" height="180" alt="" /></div>
    </div>
}

<script>
    $(function() {
        $("#div_dashboard").addClass("menuItemactive");

        $(".dash_box").find("img")
            .mouseover(function () {
                var src = $(this).attr("src").match(/[^\.]+/) + "_sc.png";
                $(this).attr("src", src);
            })
            .mouseout(function () {
                var src = $(this).attr("src").replace("_sc.png", ".png");
                $(this).attr("src", src);
            });
    });
</script>


