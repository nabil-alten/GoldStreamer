


@if (TempData.ContainsKey(GoldStreamerWebApp.Helpers.AlertStyles.Success))
{
    foreach (var value in TempData.Values)
    {
        <script>

            toastr.success("@value.ToString()");
        </script>

    }
    TempData.Remove(GoldStreamerWebApp.Helpers.AlertStyles.Success);

}

